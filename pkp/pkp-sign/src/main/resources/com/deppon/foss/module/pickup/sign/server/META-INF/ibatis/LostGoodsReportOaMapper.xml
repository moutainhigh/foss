	<!-- 查询上报oa丢货数据  -->
	<select id="queryReportOALostList" parameterType="java.lang.String" resultMap="recordLostResultMap">
 		SELECT ARR.WAYBILL_NO, HS.SERAILNO, A.TRANSFER_CENTER,ARR.OPERATE_TIME,ARR.OPERATE_ORG_CODE,
 			<include refid="Base_Column_List_Select" />
 			 FROM PKP.T_SRV_ARRIVESHEET ARR
 		INNER JOIN PKP.T_SRV_WAYBILL W
    		ON ARR.WAYBILL_NO = W.WAYBILL_NO
   			AND W.ACTIVE = 'Y'
  		LEFT JOIN BSE.T_BAS_SALES_DEPARTMENT A
    		ON A.CODE = W.LAST_LOAD_ORG_CODE
   			AND A.ACTIVE = 'Y'
   			AND A.STATION = 'Y'
 		INNER JOIN PKP.T_SRV_STAY_HANDOVERDETAIL HD
    		ON HD.WAYBILL_NO = ARR.WAYBILL_NO
   			AND HD.DELIVERBILL_NO IS NOT NULL
   			AND HD.CREATE_TIME > SYSDATE - 1
 		INNER JOIN PKP.T_SRV_STAY_HANDOVERSERIAL HS
    		ON HS.T_SRV_STAY_HANDOVERDETAIL_ID = HD.ID
 		INNER JOIN PKP.T_SRV_LABELED_GOOD L
 			ON L.WAYBILL_NO = ARR.WAYBILL_NO
   			AND L.SERIAL_NO = HS.SERAILNO
 		WHERE ARR.ACTIVE = 'Y'
   			AND ARR.DESTROYED = 'N'
   			AND ARR.STATUS = 'REFUSE'
   			AND ARR.IS_PDA_SIGN = 'Y'
   			AND ARR.OPERATE_TIME > SYSDATE - 1
   			AND L.ACTIVE = 'Y' 
   			AND NOT EXISTS
 		(SELECT 1
          	FROM PKP.T_SRV_WAYBILL_SIGN_RESULT SR
        WHERE SR.WAYBILL_NO = ARR.WAYBILL_NO
           AND SR.ACTIVE = 'Y'
           AND SR.SIGN_STATUS = 'SIGN_STATUS_ALL'
           AND SR.CREATE_TIME > SYSDATE - 1)
   		   AND W.PRODUCT_CODE NOT IN ('PACKAGE', 'RCP','EPEP', 'AF', 'PLF')
   		   AND NOT EXISTS
 	   (SELECT 1
          FROM TFR.T_OPT_STOCK S
         WHERE S.WAYBILL_NO = ARR.WAYBILL_NO
           AND S.SERIAL_NO = HS.SERAILNO
           AND (S.ORG_CODE = W.LAST_LOAD_ORG_CODE or
               S.ORG_CODE = a.transfer_center)
           )
  		   AND NOT EXISTS
 	    (SELECT 1
          FROM PKP.T_SRV_SIGN_DETAIL SD
         WHERE SD.SERIAL_NO = HS.SERAILNO
           AND SUBSTR(SD.ARRIVESHEET_NO, 0, 9) = ARR.WAYBILL_NO
           AND SD.CREATE_TIME > SYSDATE - 2)
	</select>
	
