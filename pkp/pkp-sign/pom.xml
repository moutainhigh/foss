    <build>
	    <resources>
	    	<resource>
			<directory>src/main/java</directory>
				<includes>
			         <include>**/*.xml</include>
			         <include>**/*.jrxml</include>
			         <include>**/images/*</include>
			    </includes>
			</resource> 
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*</include>
				</includes>
			</resource>			
		</resources>
		<plugins>
			<plugin>
