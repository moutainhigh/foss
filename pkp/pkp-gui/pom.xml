		<boot_version>1.0.0-SNAPSHOT</boot_version>
		<mainframe_version>1.0.0-SNAPSHOT</mainframe_version>
		<waybill_version>1.0.0-SNAPSHOT</waybill_version>
		<sysnwaybilldata_version>1.0.0-SNAPSHOT</sysnwaybilldata_version>
		<login_version>1.0.0-SNAPSHOT</login_version>
