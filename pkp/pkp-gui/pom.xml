		<!--zxy 20140515 MANA-2019 改成1.5 -->
		<!--zxy 20140313 MANA-2018 改成1.4 -->
		<framework_client_version>1.1.5-SNAPSHOT</framework_client_version>
		<!-- test lib version -->
		<junit_version>4.8.2</junit_version>
		<!-- common lib version -->
		<commons_logging_version>1.1.1</commons_logging_version>
		<log4j_version>1.2.16</log4j_version>
		<hessian_version>4.0.7</hessian_version>
		<!-- client lib version -->
		<sqlite_version>3.7.2</sqlite_version>
		<!-- URL -->
		<appHomeDirectory>../appHome</appHomeDirectory>
		<baseDirectory>.</baseDirectory>
		<javaSourceDirectory>bin</javaSourceDirectory>
		<!-- encoding -->
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

</properties>
	<build>
		<outputDirectory>bin</outputDirectory>
		<defaultGoal>compile</defaultGoal>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>${java_version}</source>
					<target>${java_version}</target>
					<encoding>${java_encoding}</encoding>
					<sourceExcludes>
						<sourceExclude>plugin.xml</sourceExclude>
					</sourceExcludes>
					<sourceIncludes>
						<sourceInclude>**/**</sourceInclude>
					</sourceIncludes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.5</version>
				<configuration>
					<encoding>${java_encoding}</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
