ALTER TABLE PKP.T_SRV_PRICING_CRITERIA_DETAIL ADD COMB_BILL_TYPE_CODE VARCHAR(50);

UPDATE PKP.T_SRV_PRICING_CRITERIA_DETAIL D1 SET D1.COMB_BILL_TYPE_CODE = 'HDP' WHERE D1.ID IN (SELECT D.ID FROM PKP.T_SRV_PRICING_CRITERIA_DETAIL D, PKP.T_SRV_PRICING_VALUATION  V  WHERE D.COMB_BILL_TYPE_CODE IS NULL AND D.PRICING_VALUATION_ID = V.ID AND V.PRODUCT_CODE = 'AIR_FREIGHT');