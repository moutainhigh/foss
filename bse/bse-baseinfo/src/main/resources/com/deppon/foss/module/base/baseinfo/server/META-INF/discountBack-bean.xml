<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		default-autowire="byName" default-lazy-init="true">

	<!-- DAO层配置 -->
	<!-- 事后折概要信息DAO-->
	<bean id="discountBackInfoDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.DiscountBackInfoDao">
	</bean>
	
	<!-- 事后折详细信息DAO-->
	<bean id="discountBackItemInfoDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.DiscountBackItemInfoDao">
	</bean>
	
	<!-- Service配置 -->
	<!-- 事后折概要信息Service -->
	<bean id="discountBackInfoService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.DiscountBackInfoService">
	</bean>
	
	<!-- 事后折详细信息Service-->
	<bean id="discountBackItemInfoService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.DiscountBackItemInfoService">
	</bean>
</beans>