<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		default-autowire="byName" default-lazy-init="true">

	<!-- dao层配置 -->
	<!-- 临欠额度区间信息 -->
	<bean id="owedLimitRegionDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.OwedLimitRegionDao">
	</bean>
	
	<!-- service层配置 -->
	<!-- 临欠额度区间信息 -->
	<bean id="owedLimitRegionService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.OwedLimitRegionService">
	</bean>
	
	<!-- action层配置 -->
	<bean id="owedLimitRegionAction" class="com.deppon.foss.module.base.baseinfo.server.action.OwedLimitRegionAction" scope="prototype">
	</bean>
</beans>