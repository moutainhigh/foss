<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		default-autowire="byName" default-lazy-init="true">

	<!-- DAO层配置 -->
	<!-- 零担事后折概要信息DAO-->
	<bean id="ltDiscountBackInfoDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.LtDiscountBackInfoDao">
	</bean>
	
	<!-- Service配置 -->
	<!-- 零担事后折概要信息Service -->
	<bean id="ltDiscountBackInfoService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.LtDiscountBackInfoService">
	</bean>

</beans>