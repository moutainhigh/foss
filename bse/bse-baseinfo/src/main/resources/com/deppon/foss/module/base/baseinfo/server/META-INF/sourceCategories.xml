<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		default-autowire="byName" default-lazy-init="true">
	<!-- 行业货源DAO -->
	
	<bean id="sourceCategoriesDao" class="com.deppon.foss.module.base.baseinfo.server.dao.impl.SourceCategoriesDao"></bean>
	
	<!-- 行业货源SERVICE -->
	<bean id="sourceCategoriesService" class="com.deppon.foss.module.base.baseinfo.server.service.impl.SourceCategoriesService"></bean>

	<!-- 行业货源ACTION-->
	<bean id="sourceCategoriesAction" class="com.deppon.foss.module.base.baseinfo.server.action.SourceCategoriesAction" scope="prototype"></bean>
</beans>