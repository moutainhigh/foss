<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
default-autowire="byName" default-lazy-init="true">

	<!-- 汇率信息配置-->
	<!-- DAO配置 -->
	<bean id="exchangeRateDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.ExchangeRateDao">
	</bean>
	<!-- Service配置 -->
	<bean id="exchangeRateService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.ExchangeRateService">
	</bean>
	
	<!-- Action配置 -->
	<bean id="exchangeRateAction"
		class="com.deppon.foss.module.base.baseinfo.server.action.ExchangeRateAction">
	</bean>
	
</beans>