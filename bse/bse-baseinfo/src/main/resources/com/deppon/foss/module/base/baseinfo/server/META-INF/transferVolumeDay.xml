<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
	default-autowire="byName" default-lazy-init="true">
	
<!-- 在线消息 -->
	<bean id="transferVolumeDayAction"
		class="com.deppon.foss.module.base.baseinfo.server.action.TransferVolumeDayAction"
		scope="prototype" />
	<!-- 数据访问层  -->
	<bean id="transferVolumeDayDao" 
			class="com.deppon.foss.module.base.baseinfo.server.dao.impl.TransferVolumeDayDao">
	</bean>	
	<!-- 业务服务层 -->
	<bean id="transferVolumeDayService" 
		  class="com.deppon.foss.module.base.baseinfo.server.service.impl.TransferVolumeDayService">
	</bean>		
	
</beans> 