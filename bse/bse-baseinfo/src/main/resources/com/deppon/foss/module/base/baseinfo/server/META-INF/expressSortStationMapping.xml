<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
	default-autowire="byName">
	<!-- 数据持久层 -->
	<bean id="expressSortStationMappingDao"
	 class="com.deppon.foss.module.base.baseinfo.server.dao.impl.ExpressSortStationMappingDao"></bean>
	<!-- 业务层 -->
	<bean id="expressSortStationMappingService" 
	class="com.deppon.foss.module.base.baseinfo.server.service.impl.ExpressSortStationMappingService"></bean>
	<!-- action -->
	<bean id="expressSortStationMappingAction"
	 class="com.deppon.foss.module.base.baseinfo.server.action.ExpressSortStationMappingAction"  scope="prototype">
	</bean>
</beans> 
 