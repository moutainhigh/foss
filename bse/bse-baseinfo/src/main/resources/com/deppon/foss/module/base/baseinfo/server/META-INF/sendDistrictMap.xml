<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
	default-autowire="byName" default-lazy-init="true">
	 <!-- 数据访问层 -->
	<bean id="sendDistrictMapDao" 
			class="com.deppon.foss.module.base.baseinfo.server.dao.impl.SendDistrictMapDao">
	</bean>	
	<!-- 业务服务层 -->
	<bean id="sendDistrictMapService" 
		  class="com.deppon.foss.module.base.baseinfo.server.service.impl.SendDistrictMapService">
	</bean>
	<!-- WEB服务ACTION层 -->
	<bean id="sendDistrictMapAction" 
		  class="com.deppon.foss.module.base.baseinfo.server.action.SendDistrictMapAction" scope="prototype">
	</bean> 
</beans> 