<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		default-autowire="byName" default-lazy-init="true">

	<!-- dao层配置 -->
	<!-- 调用存储过程dao -->
	<bean id="callProcedureDao"
		class="com.deppon.foss.module.base.baseinfo.server.dao.impl.CallProcedureDao">
	</bean>
	<!-- 调用存储过程Service -->
	<bean id="callProcedureService"
		class="com.deppon.foss.module.base.baseinfo.server.service.impl.CallProcedureService">
	</bean>
	
</beans>