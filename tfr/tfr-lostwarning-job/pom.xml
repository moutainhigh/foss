<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
		<artifactId>foss-webparent</artifactId>
		<groupId>com.deppon.foss</groupId>
		<version>0.0.1-express-SNAPSHOT</version>
		<relativePath>../../foss-webparent</relativePath>
	</parent>
  
  <artifactId>tfr-lostwarning-job</artifactId>
  <packaging>war</packaging>
  <name>tfr-lostwarning-job</name>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies>
    <dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>tfr-lostwarning-api</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency>
	<dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>tfr-lostwarning</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency>
	<dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>tfr-common</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency>
	<dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>tfr-unload</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency>
	<dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>tfr-load</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency>
<!-- 	<dependency>
		<groupId>com.deppon.foss</groupId>
		<artifactId>pkp-pricing</artifactId>
		<version>0.0.1-express-SNAPSHOT</version>
	</dependency> -->
	<dependency>
		<groupId>com.deppon.esb</groupId>
		<artifactId>esb-extends-pojo</artifactId>
		<version>${esb-version}</version>
	</dependency>
  </dependencies>
  <build>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat6-maven-plugin</artifactId>
				<configuration>
					<path>/foss</path>
					<contextReloadable>true</contextReloadable>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>ft</id>
			<properties>
				<integration.test.skip>true</integration.test.skip>
			</properties>
		</profile>
	</profiles>
</project>
