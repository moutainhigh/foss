<!DOCTYPE jboss-web PUBLIC
   "-//JBoss//DTD Web Application 5.0//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-web_5_0.dtd">
   
<jboss-web>
	<context-root>tfr-pda-itf</context-root>
	<resource-ref>
		<res-ref-name>jdbc/fossds</res-ref-name>
		<jndi-name>java:jdbc/fossds</jndi-name>
	</resource-ref>
	<resource-ref>
		<res-ref-name>jdbc/pda</res-ref-name>
		<jndi-name>java:jdbc/pda</jndi-name>
	</resource-ref>
	 
	<resource-ref>
		<res-ref-name>jms/queueManager</res-ref-name>
		<jndi-name>java:jms/queueManager</jndi-name>
	</resource-ref>
	
	<resource-ref>
		<res-ref-name>jms/performance</res-ref-name>
		<jndi-name>java:jms/performance</jndi-name>
	</resource-ref>
	
	<resource-ref>
		<res-ref-name>jms/exception</res-ref-name>
		<jndi-name>java:jms/exception</jndi-name>
	</resource-ref>
	

</jboss-web>
