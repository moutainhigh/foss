<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>foss-webparent</artifactId>
		<groupId>com.deppon.foss</groupId>
		<version>0.0.1-express-SNAPSHOT</version>
		<relativePath>../../foss-webparent</relativePath>
	</parent>

	<artifactId>tfr-execution-web</artifactId>
	<packaging>war</packaging>
	<name>tfr-execution-web</name>

	<dependencies>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>web-login</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>pkp-waybill</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
			<exclusions>
				<exclusion>
					<artifactId>slf4j-log4j12</artifactId>
					<groupId>org.slf4j</groupId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>pkp-pricing</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>bse-baseinfo</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>bse-dict</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-scheduling</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>stl-consumer</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>	
		<!-- 装卸车 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-common</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
	   <dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-load</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		 <dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>pkp-sign</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 包装 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-package</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 库存 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-stock</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 出发 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-departure</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 在途-->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-ontheway</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 到达-->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-arrival</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!--卸车 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-unload</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 结算 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>stl-common</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>stl-agency</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-management</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>bse-common</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<!-- 异常货 -->
		<dependency>
			<groupId>com.deppon.foss</groupId>
			<artifactId>tfr-exceptiongoods</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-webmvc</artifactId>
		</dependency>
		<dependency> 
			<groupId>com.deppon.foss</groupId>
			<artifactId>pkp-predeliver</artifactId>
			<version>0.0.1-express-SNAPSHOT</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat6-maven-plugin</artifactId>
				<configuration>
					<url>http://localhost:8080/manager/html</url>
					<server>tomcat-local</server>
					<path>/tfr-execution-web</path>
					<contextReloadable>true</contextReloadable>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>ft</id>
			<properties>
				<integration.test.skip>false</integration.test.skip>
			</properties>
		</profile>
	</profiles>

</project>
