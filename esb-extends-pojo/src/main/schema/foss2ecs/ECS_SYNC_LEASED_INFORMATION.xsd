				</annotation>
			</element>
			<element name="details" type="string" minOccurs="1"
				maxOccurs="unbounded">
				<annotation>
					<documentation>返回处理信息</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
</schema>  